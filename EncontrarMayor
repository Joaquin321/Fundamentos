// EncontrarMayor.cpp : Defines the entry point for the console application.
//

#include "stdafx.h"
#include <iostream>
#include "conio.h"

using namespace std;




int main()
{
	int num1, num2, num3;
	cout<<"Ingrese el numero 1: ";
	cin>>num1;
	cout<<"Ingrese el numero 2: ";
	cin>>num2;
	cout<<"Ingrese el numero 3: ";
	cin>>num3;
	if (num1>=num2)
		if (num1>=num3)
			cout<<"El numero mayor es: "<<num1<<endl;
		else 
			cout<<"El numero mayor es: "<<num3<<endl;
	else 
		if (num2>=num3)
		cout<<"El numero mayor es: "<<num2<<endl;
		else 
			cout<<"El numero mayor es: "<<num3<<endl;
	getch();

	return 0;
}
